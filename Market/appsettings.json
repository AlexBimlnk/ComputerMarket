{
  "ConnectionString": "Host=postgres_container;Port=5432;Database=market;Username=user;Password=master",

  "ImportCommandConfigurationSender": {
    "Destination": "https://import-app:5010/import/commands"
  },
  "WTCommandConfigurationSender": {
    "Destination": "https://wallet-transaction-app:5020/wt/commands"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level}] {SourceContext:l} {Scope} {Message} {NewLine}"
        }
      }
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
