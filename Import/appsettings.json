{
  "ConnectionString": "Host=postgres_container;Port=5432;Database=import;Username=user;Password=master;IncludeErrorDetail=true",

  "InternalProductSenderConfiguration": {
    "Destination": "https://market-app:5000/market/external/notification/import"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level}] {SourceContext:l} {Scope} {Message} {NewLine}"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
